{
  "name": "msks-app-kgen",
  "version": "1.0.0",
  "description": "MSKS Kiosk Generation",
  "main": "index.js",
  "scripts": {
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist main.js 0.js",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm run rimraf -- node_modules doc coverage dist compiled dll",
    "build:renderer:dev": "npm run webpack -- --config config/webpack.renderer.dev.js --progress --profile",
    "build": "npm run clean:dist && npm run build:renderer:dev",
    "build:renderer:prod": "npm run webpack -- --config config/webpack.renderer.prod.js --progress --profile",
    "build:sit": "npm run clean:dist && npm run webpack -- --config config/webpack.renderer.sit.js  --progress --profile --bail",
    "build:prod": "npm run clean:dist && npm run build:renderer:prod",
    "start": "npm run server:dev",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "npm run webpack-dev-server -- --config config/webpack.renderer.dev.js --progress --profile --watch --content-base src",
    "server": "npm run server:dev:hmr",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "rimraf": "rimraf",
    "webpack": "webpack",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "tslint": "tslint"
  },
  "author": "PCCW Solutions",
  "private": true,
